pages = {148--155},
publisher = {Elsevier B.V.},
title = {{Temperature-programmed desorption study of NO reactions on rutile {TiO}$_2$(110)-1x1}},
url = {http://dx.doi.org/10.1016/j.susc.2016.01.032},
volume = {652},
