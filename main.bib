@article{Cheng2011,
abstract = {We have used density functional theory (DFT) to investigate the adsorption and first dissociation step of ammonia on both clean and Pt-covered {TiO}$_2$ rutile (110) surfaces. On clean rutile {TiO}$_2$ (110) surface, NH3 and NH2 prefer to chemisorb on the five-coordinated titanium (Ti5c) site, and H is adsorbed favorably on the bridging oxygen (O1) site, while a single Pt atom prefers to adsorb on the hollow (H2) site. It is found that the preadsorbed Pt atom enhances significantly the adsorption of NH3, NH2 and H species on rutile {TiO}$_2$ (110) surface by binding with Pt adatom directly. It is also found that the first dissociation step of ammonia (NH3???NH2+H) on the clean rutile {TiO}$_2$ (110) surface is endothermic by 0.89eV, and no transition state exists. In addition, the first dissociation step of ammonia on Pt-covered rutile {TiO}$_2$ (110) surface is found to be endothermic by only 0.43eV, but presents an activation energy barrier of 1.82eV. A comparison of the adsorption and first dissociation step of ammonia on rutile {TiO}$_2$ (110) surface with and without Pt adatom has been discussed. Investigations indicate that the adsorbed Pt adatom on clean rutile {TiO}$_2$ (110) surface not only changes the surface electron structure, but also varies the reaction pathways by participating in the reaction. It is expected that our results would provide useful information for the development of catalyst for NH3 dissociation. ?? 2011 Elsevier B.V.},
author = {Cheng, Daojian and Lan, Jianhui and Cao, Dapeng and Wang, Wenchuan},
doi = {10.1016/j.apcatb.2011.06.010},
isbn = {0192-8651},
issn = {09263373},
journal = {Applied Catalysis B: Environmental},
keywords = {DFT calculation,Dissociation of ammonia,Platinum,Rutile {TiO}$_2$},
number = {3-4},
pages = {510--519},
pmid = {21387336},
publisher = {Elsevier B.V.},
title = {{Adsorption and dissociation of ammonia on clean and metal-covered {TiO}$_2$ rutile (110) surfaces: A comparative DFT study}},
url = {http://dx.doi.org/10.1016/j.apcatb.2011.06.010},
volume = {106},
year = {2011}
}

