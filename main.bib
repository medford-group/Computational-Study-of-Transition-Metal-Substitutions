isbn = {00219517},
issn = {00219517},
journal = {Journal of Catalysis},
keywords = {Carbon,Photocatalyst,{TiO}$_2$,Vanadium,Visible light},
number = {2},
pages = {296--302},
title = {{Highly visible-light active C- and V-doped {TiO}$_2$ for degradation of acetaldehyde}},
volume = {252},
year = {2007}
}

